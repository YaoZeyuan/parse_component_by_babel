{
  "name": "parse_compontent_by_babel",
  "version": "0.0.1",
  "description": "利用 babel 统计 node 项目中组件使用情况, 输出为json文件",
  "main": "index.js",
  "scripts": {
    "build": "babel src --config-file=./env/.babelrc --copy-files --out-dir src  --verbose --source-maps --extensions \".ts,.tsx\"",
    "watch": "babel src --config-file=./env/.babelrc --copy-files --watch --out-dir src  --verbose --source-maps --extensions \".ts,.tsx\""
  },
  "author": "yaozeyuan93@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.8.0",
    "@babel/core": "^7.7.5",
    "@babel/generator": "^7.7.4",
    "@babel/parser": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-export-default-from": "^7.7.4",
    "@babel/plugin-syntax-decorators": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-destructuring": "^7.7.4",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-flow": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@babel/traverse": "^7.7.4",
    "@babel/types": "^7.7.4",
    "lodash": "^4.17.15",
    "minimist": "^1.2.5",
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.0",
    "@babel/plugin-transform-react-jsx": "^7.7.7",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.10.0",
    "@babel/preset-typescript": "^7.8.0",
    "@types/babel__core": "^7.1.3",
    "@types/node": "^13.1.7",
    "@types/lodash": "^4.14.149"
  }
}
